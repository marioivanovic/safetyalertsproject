{
  "name": "formation-angular-advanced-starter",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "concurrently  \"ng serve\" \"npm run api\" ",
    "start:en": "ng serve --configuration=en",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "api": "json-server --watch mock_db/db.json",
    "compodoc:build": "compodoc -p tsconfig.doc.json",
    "compodoc:build-and-serve": "compodoc -p tsconfig.doc.json -s",
    "compodoc:serve": "compodoc -s",
    "extract": "ng extract-i18n --output-path src/locale",
    "dev:ssr": "ng run formation-angular-advanced-starter:serve-ssr",
    "serve:ssr": "node dist/formation-angular-advanced-starter/server/main.js",
    "build:ssr": "ng build && ng run formation-angular-advanced-starter:server",
    "prerender": "ng run formation-angular-advanced-starter:prerender"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.3.11",
    "@angular/common": "~13.3.11",
    "@angular/compiler": "~13.3.11",
    "@angular/core": "~13.3.11",
    "@angular/forms": "~13.3.11",
    "@angular/platform-browser": "~13.3.11",
    "@angular/platform-browser-dynamic": "~13.3.11",
    "@angular/platform-server": "~13.3.11",
    "@angular/router": "~13.3.11",
    "@compodoc/compodoc": "^1.1.19",
    "@fortawesome/angular-fontawesome": "^0.10.1",
    "@fortawesome/fontawesome-svg-core": "~1.2.36",
    "@fortawesome/free-regular-svg-icons": "^5.15.4",
    "@fortawesome/free-solid-svg-icons": "^5.15.4",
    "@ngrx/effects": "^13.2.0",
    "@ngrx/router-store": "^13.2.0",
    "@ngrx/store": "^13.2.0",
    "@ngrx/store-devtools": "^13.2.0",
    "@nguniversal/express-engine": "^13.1.1",
    "bootstrap": "^5.1.3",
    "express": "^4.15.2",
    "rxjs": "~7.4.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^13.3.8",
    "@angular/cli": "~13.3.8",
    "@angular/compiler-cli": "~13.3.11",
    "@angular/localize": "^13.3.11",
    "@nguniversal/builders": "^13.1.1",
    "@ngx-i18nsupport/ngx-i18nsupport": "^1.1.6",
    "@types/bootstrap": "^5.1.9",
    "@types/express": "^4.17.0",
    "@types/jasmine": "~3.10.0",
    "@types/node": "^12.11.1",
    "concurrently": "^7.2.2",
    "jasmine-core": "~3.10.0",
    "json-server": "^0.17.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "typescript": "~4.4.3"
  }
}